{
  "name": "node-elma",
  "version": "2.0.0",
  "description": "Elasto Mania NPM package to read, modify and write Elma files",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --out build/docs --excludeExternals --exclude **/*test.ts --mode modules ./src",
    "test": "jest",
    "coverage": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run test && npm run build && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmadev/node-elma.git"
  },
  "keywords": [
    "elasto mania",
    "elma",
    "replay",
    "level",
    "game"
  ],
  "author": "Roger Andersen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elmadev/node-elma/issues"
  },
  "homepage": "https://github.com/elmadev/node-elma#readme",
  "devDependencies": {
    "@types/jest": "^23.3.8",
    "@types/node": "^10.12.0",
    "coveralls": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.3"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@types/fs-extra": "^5.0.4",
    "fs-extra": "^7.0.0"
  }
}
