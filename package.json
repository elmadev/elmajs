{
  "name": "node-elma",
  "version": "0.0.1",
  "description": "Elasto Mania NPM package to read, modify and write Elma files",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --out build/docs --excludeExternals --exclude **/*test.ts --mode modules ./src",
    "test": "jest",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "coverage": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run test && npm run build && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmadev/node-elma.git"
  },
  "keywords": [
    "elasto mania",
    "elma",
    "replay",
    "level",
    "game"
  ],
  "author": "Roger Andersen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elmadev/node-elma/issues"
  },
  "homepage": "https://github.com/elmadev/node-elma#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^24.0.11",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "coveralls": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fs-extra": "^7.0.0",
    "jest": "^24.7.1",
    "prettier": "^2.0.5",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.17.7",
    "typescript": "^3.1.3"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "buffer": "^5.6.0"
  }
}
