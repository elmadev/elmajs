{
  "name": "elmajs",
  "version": "0.0.1",
  "description": "Elasto Mania NPM package to read, modify and write Elma files",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:esm && npm run build:umd",
    "build:esm": "tsc -t es6 -m es6 --outDir lib-esm",
    "build:umd": "webpack",
    "build:docs": "typedoc ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "coverage": "npm run test -- --coverage",
    "prepublishOnly": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmadev/elmajs.git"
  },
  "keywords": [
    "elasto mania",
    "elma",
    "replay",
    "level",
    "game"
  ],
  "author": "Roger Andersen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elmadev/elmajs/issues"
  },
  "homepage": "https://github.com/elmadev/elmajs#readme",
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^9.0.1",
    "jest": "^24.7.1",
    "prettier": "^2.0.5",
    "ts-jest": "^24.0.2",
    "ts-loader": "^8.0.1",
    "typedoc": "^0.20.7",
    "typescript": "^4.0.2",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0"
  },
  "files": [
    "umd",
    "lib",
    "lib-esm"
  ],
  "dependencies": {
    "buffer": "^6.0.1"
  }
}
